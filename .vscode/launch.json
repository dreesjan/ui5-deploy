{
    "configurations": [
        {
            "name": "Start ui5-deploy-test",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "cwd": "${workspaceFolder}/ui5-deploy-test",
            "windows": {
                "runtimeExecutable": "npx.cmd"
            },
            "runtimeArgs": ["fiori", "run"],
            "args": ["--open", "test/flpSandbox.html#comwagoui5deploytest-display"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "outputCapture": "std",
            "port": 9229,
            "env": {
                "DEBUG": "--inspect",
                "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false"
            }
        },
